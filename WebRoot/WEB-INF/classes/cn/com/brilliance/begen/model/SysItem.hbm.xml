<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysItem"
        table="sys_item"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="section"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[section]"
            length="250"
            unique="false"
        />
        <property
            name="label"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[label]"
            length="20"
            unique="false"
        />
        <property
            name="readonly"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[readonly]"
            length="250"
            unique="false"
        />
        <property
            name="required"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[required]"
            length="250"
            unique="false"
        />
        <property
            name="seq"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[seq]"
            length="250"
            unique="false"
        />
        <property
            name="col"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[col]"
            length="250"
            unique="false"
        />
        <property
            name="colspan"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[colspan]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="sysField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[sys_field]"
            length="250"
            unique="false"
        />
        <property
            name="relationField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[relation_field]"
            length="250"
            unique="false"
        />
        <property
            name="filterField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[filter_field]"
            length="250"
            unique="false"
        />
        <property
            name="filterMethod"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[filter_method]"
            length="250"
            unique="false"
        />
        <property
            name="fieldRelationField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[field_relation_field]"
            length="250"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="section"
			name="sectionOfSysSection"
			class="cn.com.brilliance.begen.model.SysSection"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="sys_field"
			name="sysFieldOfSysField"
			class="cn.com.brilliance.begen.model.SysField"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="relation_field"
			name="relationFieldOfSysField"
			class="cn.com.brilliance.begen.model.SysField"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="filter_field"
			name="filterFieldOfSysField"
			class="cn.com.brilliance.begen.model.SysField"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="field_relation_field"
			name="fieldRelationFieldOfSysField"
			class="cn.com.brilliance.begen.model.SysField"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
