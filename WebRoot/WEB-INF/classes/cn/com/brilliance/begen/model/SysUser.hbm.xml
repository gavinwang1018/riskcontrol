<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysUser"
        table="sys_user"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="userId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[user_id]"
            length="20"
            unique="false"
        />
        <property
            name="userName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[user_name]"
            length="50"
            unique="false"
        />
        <property
            name="password"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[password]"
            length="20"
            unique="false"
        />
        <property
            name="orgId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[org_id]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="personSex"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[person_sex]"
            length="250"
            unique="false"
        />
        <property
            name="email"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[email]"
            length="50"
            unique="false"
        />
        <property
            name="phone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[phone]"
            length="20"
            unique="false"
        />
        <property
            name="adminFlag"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[admin_flag]"
            length="250"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[remark]"
            length="250"
            unique="false"
        />
        <property
            name="deskey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[deskey]"
            length="250"
            unique="false"
        />
        <property
            name="sendCa"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[send_ca]"
            length="250"
            unique="false"
        />
        <property
            name="enableCa"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[enable_ca]"
            length="250"
            unique="false"
        />
        <property
            name="googleId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[google_id]"
            length="50"
            unique="false"
        />
        <property
            name="googlePassword"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[google_password]"
            length="20"
            unique="false"
        />
 <!-- Imported bi-directional one-to-many association to SysUserRole -->
	<set name="sysUserRoleGroup"
	     table="sys_user_role"
	     lazy="true"
	     inverse="true"
	     cascade="delete"
 	     sort="unsorted"
	>
		<key>
			<column name="user_id"/>
		</key>
		<one-to-many
			class="cn.com.brilliance.begen.model.SysUserRole"
		/>
	</set>     

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="org_id"
			name="orgIdOfSysOrg"
			class="cn.com.brilliance.begen.model.SysOrg"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
