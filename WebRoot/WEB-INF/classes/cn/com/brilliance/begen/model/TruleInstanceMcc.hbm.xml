<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.TruleInstanceMcc"
        table="t_rule_instance_mcc"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.String"
            length="32"
        >
            <generator class="native" />
        </id>
        <property
            name="ruleInstanceId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="rule_instance_id"
            length="32"
            unique="false"
        />
        <property
            name="param1"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="param_1"
            length="20"
            unique="false"
        />
        <property
            name="param2"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="param_2"
            length="20"
            unique="false"
        />
        <property
            name="param3"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="param_3"
            length="20"
            unique="false"
        />
        <property
            name="param4"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="param_4"
            length="20"
            unique="false"
        />
        <property
            name="mccCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mcc_code"
            length="20"
            unique="false"
        />
        <property
            name="riskThreshold"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="risk_threshold"
            length="20"
            unique="false"
        />
        <property
            name="bigmccCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bigmcc_code"
            length="20"
            unique="false"
        />
        <property
            name="mccId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mcc_id"
            length="32"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="rule_instance_id"
			name="ruleInstanceIdOfTruleInstance"
			class="cn.com.brilliance.begen.model.TruleInstance"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="mcc_id"
			name="mccIdOfTmcc"
			class="cn.com.brilliance.begen.model.Tmcc"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
