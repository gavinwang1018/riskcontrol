<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysWorkflowInsTask"
        table="sys_workflow_ins_task"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="operator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[operator]"
            length="40"
            unique="false"
        />
        <property
            name="taskOperator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[task_operator]"
            length="40"
            unique="false"
        />
        <property
            name="instanceStart"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="[instance_start]"
            length="250"
            unique="false"
        />
        <property
            name="instanceEnd"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="[instance_end]"
            length="250"
            unique="false"
        />
        <property
            name="instanceState"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[instance_state]"
            length="250"
            unique="false"
        />
        <property
            name="receiptOrg"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[receipt_org]"
            length="255"
            unique="false"
        />
        <property
            name="receiptRole"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[receipt_role]"
            length="255"
            unique="false"
        />
        <property
            name="receiptUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[receipt_user]"
            length="255"
            unique="false"
        />
        <property
            name="processInsId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[process_ins_id]"
            length="32"
            unique="false"
        />
        <property
            name="taskTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[task_title]"
            length="40"
            unique="false"
        />
        <property
            name="stateDefId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[state_def_id]"
            length="40"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="process_ins_id"
			name="processInsIdOfSysWorkflowInsProc"
			class="cn.com.brilliance.begen.model.SysWorkflowInsProc"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
