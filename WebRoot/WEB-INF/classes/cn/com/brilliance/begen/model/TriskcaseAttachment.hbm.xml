<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.TriskcaseAttachment"
        table="t_riskcase_attachment"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.String"
            length="32"
        >
            <generator class="native" />
        </id>
        <property
            name="riskcaseId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="riskcase_id"
            length="32"
            unique="false"
        />
        <property
            name="filename"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="filename"
            length="250"
            unique="false"
        />
        <property
            name="content"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="content"
            length="40"
            unique="false"
        />
        <property
            name="sortcode"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sortcode"
            length="250"
            unique="false"
        />
        <property
            name="caseno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="caseno"
            length="32"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="user_id"
            length="32"
            unique="false"
        />  
        <property
            name="submit"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="submit"
            length="1"
            unique="false"
        />               

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="riskcase_id"
			name="riskcaseIdOfTriskcase"
			class="cn.com.brilliance.begen.model.Triskcase"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="caseno"
			name="casenoOfTriskcase"
			class="cn.com.brilliance.begen.model.Triskcase"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
