<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysDailyIns"
        table="sys_daily_ins"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="eventType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[event_type]"
            length="32"
            unique="false"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[title]"
            length="50"
            unique="false"
        />
        <property
            name="url"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[url]"
            length="250"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[description]"
            length="255"
            unique="false"
        />
        <property
            name="defineDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="[define_date]"
            length="250"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[user_id]"
            length="32"
            unique="false"
        />
        <property
            name="shareFlag"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[share_flag]"
            length="250"
            unique="false"
        />
        <property
            name="shareData"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[share_data]"
            length="1000"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="user_id"
			name="userIdOfSysUser"
			class="cn.com.brilliance.begen.model.SysUser"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
