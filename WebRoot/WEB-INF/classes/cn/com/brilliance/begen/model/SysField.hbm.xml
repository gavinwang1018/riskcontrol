<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysField"
        table="sys_field"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[name]"
            length="50"
            unique="false"
        />
        <property
            name="label"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[label]"
            length="20"
            unique="false"
        />
        <property
            name="standard"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[standard]"
            length="250"
            unique="false"
        />
        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[type]"
            length="250"
            unique="false"
        />
        <property
            name="formula"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[formula]"
            length="250"
            unique="false"
        />
        <property
            name="checked"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[checked]"
            length="250"
            unique="false"
        />
        <property
            name="length"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[length]"
            length="250"
            unique="false"
        />
        <property
            name="numDecimal"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[num_decimal]"
            length="250"
            unique="false"
        />
        <property
            name="dataObject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[data_object]"
            length="250"
            unique="false"
        />
        <property
            name="lookupParameter"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[lookup_parameter]"
            length="250"
            unique="false"
        />
        <property
            name="mask"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[mask]"
            length="20"
            unique="false"
        />
        <property
            name="viewSize"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[view_size]"
            length="250"
            unique="false"
        />
        <property
            name="lookupFieldType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[lookup_field_type]"
            length="250"
            unique="false"
        />
        <property
            name="defaultValue"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[default_value]"
            length="100"
            unique="false"
        />
        <property
            name="editFlag"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[edit_flag]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="selectItem"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[select_item]"
            length="250"
            unique="false"
        />
        <property
            name="visibleCols"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[visible_cols]"
            length="250"
            unique="false"
        />
        <property
            name="relationObject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[relation_object]"
            length="250"
            unique="false"
        />
        <property
            name="autoNumberStart"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[auto_number_start]"
            length="250"
            unique="false"
        />
        <property
            name="autoNumberFormat"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[auto_number_format]"
            length="250"
            unique="false"
        />
        <property
            name="formulaReturnType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[formula_return_type]"
            length="250"
            unique="false"
        />
        <property
            name="primateKey"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[primate_key]"
            length="250"
            unique="false"
        />
        <property
            name="visibleLines"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="[visible_lines]"
            length="250"
            unique="false"
        />
        <property
            name="indexFlag"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[index_flag]"
            length="250"
            unique="false"
        />
        <property
            name="encryptFlag"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[encrypt_flag]"
            length="250"
            unique="false"
        />
        <property
            name="smartcardType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[smartcard_type]"
            length="250"
            unique="false"
        />
        <property
            name="sysRecordOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sys_record_order"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="data_object"
			name="dataObjectOfSysDataObject"
			class="cn.com.brilliance.begen.model.SysDataObject"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="relation_object"
			name="relationObjectOfSysDataObject"
			class="cn.com.brilliance.begen.model.SysDataObject"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
