<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.TruleInstance"
        table="t_rule_instance"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.String"
            length="32"
        >
            <generator class="native" />
        </id>
        <property
            name="enable"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="enable"
            length="250"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
            length="100"
            unique="false"
        />
        <property
            name="cycle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cycle"
            length="20"
            unique="false"
        />
        <property
            name="timeParam"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="time_param"
            length="20"
            unique="false"
        />
        <property
            name="needTransInfo"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="need_trans_info"
            length="250"
            unique="false"
        />
        <property
            name="supportTrace"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="support_trace"
            length="250"
            unique="false"
        />
        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
            length="250"
            unique="false"
        />
        <property
            name="versionBm"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="version_bm"
            length="20"
            unique="false"
        />
        <property
            name="transInfo"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="trans_info"
            length="250"
            unique="false"
        />
        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="code"
            length="20"
            unique="false"
        />
        <property
            name="beginTraceDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="begin_trace_date"
            length="250"
            unique="false"
        />
        <property
            name="belongToInst"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="belong_to_inst"
            length="32"
            unique="false"
        />
        <property
            name="endTraceDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="end_trace_date"
            length="250"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="belong_to_inst"
			name="belongToInstOfTorganizeInfo"
			class="cn.com.brilliance.begen.model.TorganizeInfo"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
