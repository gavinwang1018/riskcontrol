<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.TkeoMchnt"
        table="t_keo_mchnt"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.String"
            length="32"
        >
            <generator class="native" />
        </id>
        <property
            name="mchntCd"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mchnt_cd"
            length="255"
            unique="false"
        />
        <property
            name="reason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="reason"
            length="255"
            unique="false"
        />
        <property
            name="mchntName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mchnt_name"
            length="255"
            unique="false"
        />
        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
            length="250"
            unique="false"
        />
        <property
            name="belongToInst"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="belong_to_inst"
            length="250"
            unique="false"
        />
        <property
            name="settlementAccntNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="settlement_accnt_no"
            length="255"
            unique="false"
        />
        <property
            name="state"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="20"
            unique="false"
        />
        <property
            name="approvedDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="approved_date"
            length="250"
            unique="false"
        />
        <property
            name="withdrawInputdate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="withdraw_inputdate"
            length="250"
            unique="false"
        />
        <property
            name="mchntId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mchnt_id"
            length="255"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="belong_to_inst"
			name="subInst"
			class="cn.com.brilliance.begen.model.TorganizeInfo"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
