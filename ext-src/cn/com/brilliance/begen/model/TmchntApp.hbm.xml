<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.TmchntApp"
        table="t_mchnt_app"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.String"
            length="32"
        >
            <generator class="native" />
        </id>
        <property
            name="mchntId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mchnt_id"
            length="32"
            unique="false"
        />
        <property
            name="apptype"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="apptype"
            length="255"
            unique="false"
        />
        <property
            name="main"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="main"
            length="250"
            unique="false"
        />
        <property
            name="acquirer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="acquirer"
            length="255"
            unique="false"
        />
        <property
            name="networktransferinst"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="networktransferinst"
            length="255"
            unique="false"
        />
        <property
            name="memberinstmerno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="memberinstmerno"
            length="15"
            unique="false"
        />
        <property
            name="depositbank"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="depositbank"
            length="255"
            unique="false"
        />
        <property
            name="settlementaccntname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="settlementaccntname"
            length="255"
            unique="false"
        />
        <property
            name="settlementaccntno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="settlementaccntno"
            length="21"
            unique="false"
        />
        <property
            name="belongToInst"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="belong_to_inst"
            length="250"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->	
		<many-to-one
			column="mchnt_id"
			name="mchntIdOfTmchnt"
			class="cn.com.brilliance.begen.model.Tmchnt"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="belong_to_inst"
			name="belongToInstOfTorganizeInfo"
			class="cn.com.brilliance.begen.model.TorganizeInfo"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
