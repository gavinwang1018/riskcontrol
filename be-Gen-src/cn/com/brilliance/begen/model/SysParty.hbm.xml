<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysParty"
        table="sys_party"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="partyId"
            column="party_id"
            type="java.lang.String"
            length="20"
        >
            <generator class="assigned" />
        </id>
        <property
            name="partyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="party_name"
            length="255"
            unique="false"
        />
        <property
            name="databaseName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="database_name"
            length="50"
            unique="false"
        />
        <property
            name="lastDeployTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="last_deploy_time"
            length="250"
            unique="false"
        />
        <property
            name="contractPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="contract_person"
            length="50"
            unique="false"
        />
        <property
            name="email"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="email"
            length="100"
            unique="false"
        />
        <property
            name="phone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="phone"
            length="20"
            unique="false"
        />
        <property
            name="userNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="user_number"
            length="250"
            unique="false"
        />
        <property
            name="openFlag"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="open_flag"
            length="250"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="250"
            unique="false"
        />
        <property
            name="logo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="logo"
            length="250"
            unique="false"
        />
        <property
            name="developMode"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="develop_mode"
            length="250"
            unique="false"
        />
        <property
            name="enableCa"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="enable_ca"
            length="250"
            unique="false"
        />
        <property
            name="sendCa"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="send_ca"
            length="250"
            unique="false"
        />
        <property
            name="version"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="version"
            length="10"
            unique="false"
        />
        <property
            name="deskey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="deskey"
            length="250"
            unique="false"
        />
        <property
            name="dbSpace"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="db_space"
            length="250"
            unique="false"
        />
        <property
            name="indexSpace"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="index_space"
            length="250"
            unique="false"
        />
        <property
            name="uploadSpace"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="upload_space"
            length="250"
            unique="false"
        />
        <property
            name="totalSpace"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="total_space"
            length="250"
            unique="false"
        />
        <property
            name="industry"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="industry"
            length="250"
            unique="false"
        />
        <property
            name="area"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="area"
            length="250"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
    </class>
</hibernate-mapping>
