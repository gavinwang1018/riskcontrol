<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysListField"
        table="sys_list_field"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="listLayout"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[list_layout]"
            length="250"
            unique="false"
        />
        <property
            name="view"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[view]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="sortFlag"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[sort_flag]"
            length="250"
            unique="false"
        />
        <property
            name="queryMode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[query_mode]"
            length="250"
            unique="false"
        />
        <property
            name="sysField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[sys_field]"
            length="250"
            unique="false"
        />
        <property
            name="relationField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[relation_field]"
            length="250"
            unique="false"
        />
        <property
            name="sysRecordOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sys_record_order"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="list_layout"
			name="listLayoutOfSysListLayout"
			class="cn.com.brilliance.begen.model.SysListLayout"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="relation_field"
			name="relationFieldOfSysField"
			class="cn.com.brilliance.begen.model.SysField"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
