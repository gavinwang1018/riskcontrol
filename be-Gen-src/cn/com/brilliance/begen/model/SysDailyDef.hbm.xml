<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysDailyDef"
        table="sys_daily_def"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="eventType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[event_type]"
            length="250"
            unique="false"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[title]"
            length="100"
            unique="false"
        />
        <property
            name="url"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[url]"
            length="1000"
            unique="false"
        />
        <property
            name="content"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[content]"
            length="250"
            unique="false"
        />
        <property
            name="dataObject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[data_object]"
            length="250"
            unique="false"
        />
        <property
            name="dateField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[date_field]"
            length="250"
            unique="false"
        />
        <property
            name="filterExpress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[filter_express]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="urlDescription"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[url_description]"
            length="255"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="event_type"
			name="eventTypeOfSysDailyEvent"
			class="cn.com.brilliance.begen.model.SysDailyEvent"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="data_object"
			name="dataObjectOfSysDataObject"
			class="cn.com.brilliance.begen.model.SysDataObject"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
