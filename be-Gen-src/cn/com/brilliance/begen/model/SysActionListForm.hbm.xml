<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class
        name="cn.com.brilliance.begen.model.SysActionListForm"
        table="sys_action_list_form"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="[id]"
            type="java.lang.String"
            length="32"
        >
            <generator class="uuid.hex" />
        </id>
        <property
            name="sysListLayout"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[sys_list_layout]"
            length="250"
            unique="false"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[name]"
            length="20"
            unique="false"
        />
        <property
            name="label"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[label]"
            length="50"
            unique="false"
        />
        <property
            name="authority"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[authority]"
            length="250"
            unique="false"
        />
        <property
            name="url"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[url]"
            length="1000"
            unique="false"
        />
        <property
            name="target"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[target]"
            length="250"
            unique="false"
        />
        <property
            name="pageCheck"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[page_check]"
            length="250"
            unique="false"
        />
        <property
            name="partyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[party_id]"
            length="20"
            unique="false"
        />
        <property
            name="urlDescription"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[url_description]"
            length="255"
            unique="false"
        />
        <property
            name="disableExpress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[disable_express]"
            length="255"
            unique="false"
        />
        <property
            name="confirmOperate"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="[confirm_operate]"
            length="250"
            unique="false"
        />
        <property
            name="confirmMessage"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="[confirm_message]"
            length="250"
            unique="false"
        />
        <property
            name="sysRecordOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sys_record_order"
            unique="false"
        />

<!-- imported bi-directional many-to-one association -->
		<many-to-one
			column="sys_list_layout"
			name="sysListLayoutOfSysListLayout"
			class="cn.com.brilliance.begen.model.SysListLayout"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
		<many-to-one
			column="party_id"
			name="partyIdOfSysParty"
			class="cn.com.brilliance.begen.model.SysParty"
            not-null="false"
	        update="false"
	        insert="false"
			not-found="ignore"
			lazy="proxy"
		/>	
    </class>
</hibernate-mapping>
